{
    "name": "yiisoft/yii2-app-advanced",
    "description": "Yii 2 Advanced Project Template",
    "keywords": ["yii2", "framework", "advanced", "project template"],
    "homepage": "http://www.yiiframework.com/",
    "type": "project",
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/yiisoft/yii2/issues?state=open",
        "forum": "http://www.yiiframework.com/forum/",
        "wiki": "http://www.yiiframework.com/wiki/",
        "irc": "irc://irc.freenode.net/yii",
        "source": "https://github.com/yiisoft/yii2"
    },
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.4.0",
        "yiisoft/yii2": ">=2.0.7",
        "yiisoft/yii2-bootstrap": "*",
        "bower-asset/drmonty-chosen": "*",
        "yiisoft/yii2-swiftmailer": "*",
        "yiisoft/yii2-jui": "^2.0",
        "kartik-v/yii2-widget-datetimepicker": "*",
        "2amigos/yii2-table-export-widget": "*",
        "webtoucher/yii2-amqp": "*",
        "devmustafa/yii2-amqp": "dev-master",
        "reportico/yii2-reportico": "dev-master",
        "yii2mod/yii2-chosen-select": "dev-master",
        "clevertech/yii-booster": "v3.0.0",
        "bower-asset/clipboard": "*",
        "uran1980/yii2-bootstrap-confirmation": "dev-master",
        "mdmsoft/yii2-admin": "2.x-dev",
        "kartik-v/yii2-grid": "@dev",
        "kartik-v/yii2-mpdf": "@dev",
        "2amigos/yii2-file-upload-widget": "~1.0",
        "kartik-v/yii2-editable": "@dev",
        "kartik-v/yii2-widget-colorinput": "@dev",
        "kartik-v/yii2-widget-switchinput": "@dev",
        "kartik-v/yii2-widget-typeahead": "@dev",
        "2amigos/yii2-bootstrap-form-helpers-library": "*",
        "kartik-v/yii2-dropdown-x": "*",
        "kartik-v/yii2-widget-select2": "@dev",
        "kartik-v/yii2-popover-x": "*",
        "kartik-v/dependent-dropdown": "dev-master",
        "kartik-v/yii2-widget-depdrop": "@dev",
        "kartik-v/yii2-tabs-x": "@dev",
        "ramosisw/yii2-material-dashboard": "dev-master"
    },
    "require-dev": {
        "yiisoft/yii2-codeception": "*",
        "yiisoft/yii2-debug": "*",
        "yiisoft/yii2-gii": "*",
        "yiisoft/yii2-faker": "*"
    },
    "config": {
    "process-timeout": 1800,
      "fxp-asset": {
        "repositories": {
          "bower-asset/eve": {
          "type": "bower-vcs",
          "url": "https://github.com/adobe-webplatform/eve.git"
          }
        }
      }
  },
    "extra": {
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        }
    }
}
